    "from sklearn.preprocessing import OneHotEncoder, LabelEncoder\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "OneHotEncoder(categorical_features='all', dtype=<type 'numpy.float64'>,\n",
      "       handle_unknown='ignore', n_values='auto', sparse=True)\n"
     ]
    }
   ],
   "source": [
    "X = ['NYC', \"ABC\"]\n",
    "y = [0,1]\n",
    "le = LabelEncoder()\n",
    "le.fit(X)\n",
    "\n",
    "new_X = le.transform(X)\n",
    "enc = OneHotEncoder(handle_unknown='ignore')\n",
    "X_one_hot = enc.fit([new_X])\n",
    "print(X_one_hot)\n"
   ]
  }
